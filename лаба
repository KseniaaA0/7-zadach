#include <iostream> //библиотека ввода вывода 
 
using std::cout; //подключаем вывод 
using std::cin; //подключаем ввод 
     
int main() { 
     
    int x1,x2,y1,y2; //объявляем переменные 
    cout << "x1: ";  
    cin >> x1; //вводим 1ое значение 
 
    cout << "x2: "; 
    cin >> x2; //вводим 2ое значение 
 
    cout << "y1: "; 
    cin >> y1; //вводим 3ое значение 
 
    cout << "y2: "; 
    cin >> y2; //вводим 4ое значение 
 
    cout << (x1 * x2) + (y1 * y2) << "\n";  //считаем по формуле 
    return 0; 
}
