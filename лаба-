#include <iostream> //библиотека ввода вывода  
 
int main() { 
 
    using std::cout;; //подключаем вывод  
    using std::cin;; //подключаем ввод  
     
    int num,n1,n2,n3,n4,n5; //объявляем переменные  
    cout<<"Введите 5 значное число - "<<"\n"; //просим ввести число 5значное 
    cin >> num; //записываем число в переменную 
    n1 = num/10000; //получаем первое число 
    n2 = (num/1000)%10; //получаем второе число 
    n3 = (num/100)%10; //получаем третье число 
    n4 = (num/10)%10; //получаем четвертое число 
    n5 = num%10; //получаем пятое число 
    cout << ((n1*100) + (n2*10) + n3) /((n4*10) + n5); // первие 3 числа делим на последние 2 
    return 0; 
}
