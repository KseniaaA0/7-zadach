#include <iostream>
 using namespace std;
  int main(){
  setlocale(LC_ALL, "russia");

  int n;

  cout << "Введите количество чисел, которое вы хотите ввести: ";
  cin >> n;

  cout << "Введите " << n << " чисел: "; 
                                        
  int *dinamich_array = (int*)malloc(n * sizeof(int)); // динамический массив(int указываем что наш массив будет из чисел,с помощью malloc выделяем память для массива sizeof(int) для чисел умножаем на количество элементов
  for (int i = 0; i < n; i++) { 
    cin >> dinamich_array[i];  // считываем числа в ячейки массива
  }
  int i, j ;{
}
  for (int i = 0; i < 10; i++) { //цикл , получается числа от 0 до 10
    cout<<i;
  }
    for (int j = 0; j < 9; j++) {
      cout<<j;
    }
    {
      if (dinamich_array[j] > dinamich_array[j + 1]) { //происходит сравнивание двух элементов.Если  результат этого условия положительный, то мы меняем значение элементов.Если же результат отрицателен, то мы пропускаем этап смены значений.
        int b = dinamich_array[j]; //создали дополнительную переменную
        dinamich_array[j] = dinamich_array[j + 1]; //меняем местами
        dinamich_array[j + 1] = b; //значение элементов
      }
    }

  return 0;
}
